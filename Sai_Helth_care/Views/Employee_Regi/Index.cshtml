
@{
    ViewBag.Title = "Index";
}

<style>
    .table td, .table th {
        padding: 0.50rem;
        vertical-align: top;
        border-top: 1px solid #dee2e6;
        font-size: 12px;
    }
</style>

<style>
    .table td, .table th {
        padding: 0.50rem;
        vertical-align: top;
        border-top: 1px solid #dee2e6;
        font-size: 12px;
    }

    #example-table thead {
        background-color: #BE7833;
        position: sticky;
        z-index: 0;
        color: white;
        top: 0;
    }

    .table-responsive {
        max-height: 400px;
        overflow-y: auto;
    }
</style>


<style type="text/css">
    body {
        font-family: Arial;
        font-size: 10pt;
    }

    .error {
        color: Red;
    }

    #BANK_NAME {
        text-transform: uppercase;
    }

        #BANK_NAME::placeholder {
            text-transform: none;
        }

    #IFSC_CODE {
        text-transform: uppercase;
    }

        #IFSC_CODE::placeholder {
            text-transform: none;
        }

    #PAN {
        text-transform: uppercase;
    }

        #PAN::placeholder {
            text-transform: none;
        }
</style>


<section ng-controller="EmployeeCtrl">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-1">
                                <a ng-click="GoToPreviousNextPage('Previous')" class="btn btn-xs" style="background-color: green; color: white "><span class="fa fa-arrow-left"></span></a>
                            </div>
                            <div class="col-lg-9" style="margin-left:-50px">
                                <h3 class="card-title"><b>Employee Master</b></h3>
                            </div>
                            <div class="col-lg-2" style="text-align:center">
                                <button type="button" class="btn btn-sm pull-left" style="background-color: green; color: white" data-toggle="modal" ng-click="AdminClick()">Add Employee</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div div class="col-lg-2">
                            <input type="text" name="Service" id="STARTING_DATE1" class="form-control" placeholder="Select From Date" />
                        </div>
                        <div div class="col-lg-2">
                            <input type="text" name="Service" id="ENDING_DATE1" class="form-control" placeholder="Select To Date" />
                        </div>

                        <div class="col-1" style="padding:3px;">
                            <button type="button" class="btn btn-sm pull-left" style="background-color: green; color: white" ng-click="GetData()">Get Data</button>
                        </div>
                        @*<div class="col-1" style="padding:3px;">
                                 <button type="button" class="btn btn-sm pull-left" style="background-color: green; color: white" data-toggle="modal" data-target="#Comp_add">Export</button>
                            </div>*@
                        <div class="col-md-4"></div>
                        <div class="col-3">
                            <input type="text" class="form-control" placeholder="Search Employee" ng-model="FARMER_SEARCH" ng-keyup="SearchAdmin()">
                        </div>
                    </div>
                    <br>
                    <div class="table-responsive">
                        <table class="table table-bordered" id="example-table" style="font-size: 0.9rem;color:black;">
                            <thead>
                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                    <th>Emp Id</th>
                                    <th>Full Name</th>
                                    <th>Contact No</th>
                                    @*<th>Current Address</th>*@
                                    <th> Permanent Address</th>
                                    <th>Department </th>
                                    <th>Designation </th>
                                    <th>Company Name </th>
                                    @*<th>City</th>*@
                                    <th>Status</th>
                                    <th>Reg Date</th>
                                    <th style="height:15%;width:10%;">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="Admin in EmployeeList">
                                    <td>{{page*30+$index+1}}</td>
                                    @*<td><a href="~/Employee_Regi/EmployeeDetails/{{Admin.EMP_ID}}">{{Admin.EMP_NAME}}</a> </td>*@
                                    <td>{{Admin.EMP_NAME}}</td>
                                    <td>{{Admin.CONTACT_NO}}</td>
                                    <td>{{Admin.PERMENENT_ADDRESS}}</td>
                                    <td>{{Admin.DEP_NAME}}</td>
                                    <td>{{Admin.DESI_NAME}}</td>
                                    <td>{{Admin.COMPANY_NAME}}</td>
                                    <td><a href="" ng-confirm-click="Do you want to change status ?" confirmed-click="ChangeStatus(Admin)"><b ng-style="Admin.STATUS=='Active'?{'color':'green'}:{'color':'red'}">{{Admin.STATUS}}</b></a></td>
                                    <td>{{Admin.REG_DATE}}</td>
                                    <td>
                                        <a data-toggle="modal" ng-click="getForUpdate(Admin)" title="Update" class="btn btn-xs btn-danger">Edit </a>
                                        <button type="button" class="btn btn-xs btn-success" ng-click="GetAdminPermision(Admin)" data-target="#Module" style="background-color: green;color: white" data-toggle="modal">Permission</button>
                                        <a href="~/Employee_Regi/TourMap?EMP_ID={{Admin.EMP_ID}}" ng-show="Admin.DEP_NAME === 'Service Engineer'" style="color:black" title="View" class="btn btn-xs btn-primary">View </a>

                                    </td>

                                </tr>
                                <tr ng-if="!pager.pages.length">
                                    <td colspan="10" style="color:black; text-align:center;"><h5><b>No record found!</b></h5></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <ul ng-if="pager.pages.length" class="pagination" style="margin-left:30px;">
                        <li ng-class="{disabled:pager.currentPage === 1}">
                            <a ng-click="setPage(1)">First</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === 1}">
                            <a ng-click="setPage(pager.currentPage - 1)">Previous</a>
                        </li>
                        <li ng-repeat="page in pager.pages" ng-class="{active:pager.currentPage === page}">
                            <a ng-click="setPage(page)">{{page}}</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === pager.totalPages}">
                            <a ng-click="setPage(pager.currentPage + 1)">Next</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === pager.totalPages}">
                            <a ng-click="setPage(pager.totalPages)">Last</a>
                        </li>
                    </ul>
                </div>
                <div class="modal fade" id="Admin_Addupdate">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header" style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: white; padding: 10px; ">
                                <h4 class="modal-title"><b>{{Admin_Action}}</b></h4>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <div class="card-body">
                                    <form name="EmployeeAddEdit" ng-submit="EmployeeAddEdit(EmployeeAddEdit.$valid)" data-toggle="validator" role="form">
                                        <div class="row">


                                            <div class="form-group col-md-6">
                                                <label class="control-label">Full Name:</label><label style="color:red;">*</label>
                                                <input type="text" id="EMP_NAME" name="EMP_NAME" ng-model="EMP_NAME" maxlength="100" ng-pattern="/^[A-Za-z][A-Za-z\s]*$/" class="form-control" placeholder="Enter Full Name" required />
                                                <span ng-show="(EmployeeAddEdit.EMP_NAME.$invalid && (EmployeeAddEdit.EMP_NAME.$touched || EmployeeAddEdit.EMP_NAME.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.EMP_NAME.$error.required">Full Name is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.EMP_NAME.$error.pattern">Enter valid Full Name</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">Contact No.:</label><label style="color:red;">*</label>
                                                <input type="text" name="CONTACT_NO" ng-model="CONTACT_NO" onkeydown="if(this.key==='.' || this.key==='-' ||  this.key==='+' ){this.preventDefault();} javascript: return this.keyCode == 69 ? false : true" class="form-control ng-pristine ng-valid ng-touched" id="txt_contact_no" autocomplete="off" ng-pattern="/^([6-9]\d*)$/" onkeypress="if(this.value.length==10) return false; " onpaste="return false" minlength="10" maxlength="10" placeholder="Enter Contact No" required />
                                                <span ng-show="(EmployeeAddEdit.CONTACT_NO.$invalid && (EmployeeAddEdit.CONTACT_NO.$touched || EmployeeAddEdit.CONTACT_NO.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.CONTACT_NO.$error.required">Contact No. is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.CONTACT_NO.$error.number">Enter numbers only</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.CONTACT_NO.$error.pattern">Enter valid Contact No. </span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label"> Alternate Contact No.:</label>
                                                <input type="text" name="ALTERNATE_CONT_NO" ng-model="ALTERNATE_CONT_NO" id="txt_contact_no2" onkeydown="if(this.key==='.' || this.key==='-' ||  this.key==='+' ){this.preventDefault();} javascript: return this.keyCode == 69 ? false : true" class="form-control ng-pristine ng-valid ng-touched" autocomplete="off" maxlength="10" ng-pattern="/^([6-9]\d*)$/" onkeypress="if(this.value.length==10) return false;" onpaste="return false" placeholder="Enter Alternate Contact No" />
                                                <span ng-show="(EmployeeAddEdit.ALTERNATE_CONT_NO.$invalid && (EmployeeAddEdit.ALTERNATE_CONT_NO.$touched || EmployeeAddEdit.ALTERNATE_CONT_NO.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.ALTERNATE_CONT_NO.$error.required">Alternate Contact No. is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ALTERNATE_CONT_NO.$error.number">Enter numbers only</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ALTERNATE_CONT_NO.$error.pattern">Enter valid Alternate Contact No. </span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Email:</label><label style="color:red;">*</label>
                                                <input type="email" id="EMAIL" name="EMAIL" ng-model="EMAIL" maxlength="100" class="form-control" autocomplete="off" onkeyup="if(this.value.length==100) return false;" placeholder="Enter Email" required />
                                                <span ng-show="( EmployeeAddEdit.EMAIL.$invalid && (EmployeeAddEdit.EMAIL.$touched || EmployeeAddEdit.EMAIL.$dirty ))">
                                                    <span class="error" ng-show="EmployeeAddEdit.EMAIL.$error.required">Email Id is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.EMAIL.$error.email">Please enter a valid email.</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Alternate Email:</label>
                                                <input type="email" id="ALTERNATE_EMAIL" name="ALTERNATE_EMAIL" maxlength="100" ng-model="ALTERNATE_EMAIL" autocomplete="off" class="form-control" placeholder="Enter Alternate Email" />
                                                <span ng-show="(EmployeeAddEdit.ALTERNATE_EMAIL.$invalid && (EmployeeAddEdit.ALTERNATE_EMAIL.$touched || EmployeeAddEdit.ALTERNATE_EMAIL.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.ALTERNATE_EMAIL.$error.required">Alternate Email Id is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ALTERNATE_EMAIL.$error.email">Please enter a valid Alternate email.</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Password :</label><label style="color:red;">*</label>
                                                <div class="input-group auth-pass-inputgroup mb-3">
                                                    <input class="form-control pe-5 password-input" type="password" name="PASSWORD" id="PASSWORD" maxlength="15" ng-model="PASSWORD" placeholder="Enter Password" required />
                                                    <div class="input-group-append">
                                                        <span class="input-group-text toggle-password" onclick="togglePasswordVisibility('PASSWORD')">
                                                            <i class="fa fa-eye" id="eye-icon"></i>
                                                        </span>
                                                    </div>
                                                </div>
                                                <span ng-show="(EmployeeAddEdit.PASSWORD.$invalid && (EmployeeAddEdit.PASSWORD.$touched || EmployeeAddEdit.PASSWORD.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.PASSWORD.$error.required">Password is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.PASSWORD.$error.password">Please enter a valid Password</span>
                                                </span>
                                            </div>




                                            <div class="form-group col-md-6">
                                                <label class="control-label">Date Of Birth:</label><label style="color:red;">*</label>
                                                <input type="text" id="EMP_DOB" name="EMP_DOB" maxlength="10" class="form-control" ng-model="EMP_DOB" placeholder="Select Date Of Birth" />
                                                <span ng-show="(EmployeeAddEdit.EMP_DOB.$invalid && (EmployeeAddEdit.EMP_DOB.$touched || EmployeeAddEdit.EMP_DOB.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.EMP_DOB.$error.required">Date Of Birth is required</span>
                                                    @* <span class="error" ng-show="EmployeeAddEdit.EMP_DOB.$error.date">Invalid Date Of Birth</span>*@
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Department:</label>
                                                <select ng-init="DEPARTMENT_ID" id="DEPARTMENT_ID" class="form-control" name="DEPARTMENT_ID" ng-model="DEPARTMENT_ID"
                                                        ng-options="Depart.DEP_ID as Depart.DEP_NAME for Depart in DepartmentList"
                                                        ng-selected="Depart.DEP_ID == DEPARTMENT_ID" ng-change="DepartmentChange()">
                                                    <option value="">Select Department</option>
                                                </select>
                                                @*<select ng-model="DEPARTMENT_ID" id="DEPARTMENT_ID" name="DEPARTMENT_ID" class="form-control" ng-change="DepartmentChange()">
                                                        <option value="">Select Department</option>
                                                        <option ng-repeat="Depart in DepartmentList" value="{{Depart.DEP_ID}}">{{Depart.DEP_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(EmployeeAddEdit.DEPARTMENT_ID.$invalid && (EmployeeAddEdit.DEPARTMENT_ID.$touched || EmployeeAddEdit.DEPARTMENT_ID.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.DEPARTMENT_ID.$error.required">Department is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Designation:</label>
                                                <select ng-init="DESIGNATION_ID" id="DESIGNATION_ID" class="form-control" name="DESIGNATION_ID" ng-model="DESIGNATION_ID"
                                                        ng-options="Desig.DESI_ID as Desig.DESI_NAME for Desig in DesignationList"
                                                        ng-selected="Desig.DESI_ID == DESIGNATION_ID">
                                                    <option value="">Select Designation</option>
                                                </select>
                                                @*<select ng-model="DESIGNATION_ID" id="DESIGNATION_ID" name="DESIGNATION_ID" class="form-control">
                                                        <option value="">Select Designation</option>
                                                        <option ng-repeat="Desig in DesignationList" value="{{Desig.DESI_ID}}">{{Desig.DESI_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(EmployeeAddEdit.DESIGNATION_ID.$invalid && (EmployeeAddEdit.DESIGNATION_ID.$touched || EmployeeAddEdit.DESIGNATION_ID.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.DESIGNATION_ID.$error.required">Designation is required</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6" ng-show="DESIGNATION_ID!=13&&DEPARTMENT_ID!=17">
                                                <label class="control-label">Company Name:</label><label style="color:red;">*</label>
                                                <select ng-init="COMPANY_ID" id="COMPANY_ID" class="form-control" name="COMPANY_ID" ng-model="COMPANY_ID"
                                                        ng-options="Company.COMPANY_ID as Company.COMPANY_NAME for Company in CompanyList"
                                                        ng-selected="Company.COMPANY_ID == COMPANY_ID" >
                                                    <option value="">Select Company Name</option>
                                                </select>
                                                @*<select id="COMPANY_ID" name="COMPANY_ID" class="form-control" ng-model="COMPANY_ID" required>
                                                        <option value="">Select Company Name </option>
                                                        <option ng-repeat="Company in CompanyList" value="{{Company.COMPANY_ID}} ">{{Company.COMPANY_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(EmployeeAddEdit.COMPANY_ID.$invalid && (EmployeeAddEdit.COMPANY_ID.$touched || EmployeeAddEdit.COMPANY_ID.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.COMPANY_ID.$error.required">Company Name is required</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">Permanent Address:</label>
                                                <textarea id="PERMENENT_ADDRESS" name="PERMENENT_ADDRESS" ng-model="PERMENENT_ADDRESS" maxlength="300" class="form-control" placeholder="Enter Permanent Address"></textarea>
                                                <span ng-show="(EmployeeAddEdit.PERMENENT_ADDRESS.$invalid && (EmployeeAddEdit.PERMENENT_ADDRESS.$touched || EmployeeAddEdit.PERMENENT_ADDRESS.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.PERMENENT_ADDRESS.$error.required">Permanent Address is required</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">State:</label><label style="color:red;">*</label>
                                                <select ng-init="STATE_ID" id="STATE_ID" class="form-control" name="STATE_ID" ng-model="STATE_ID"
                                                        ng-options="state.STATE_ID as state.STATE_NAME for state in StateList"
                                                        ng-selected="state.STATE_ID == STATE_ID" ng-change="GetstateChange()" required>
                                                    <option value="">Select State</option>
                                                </select>
                                                @*<select ng-model="STATE_ID" id="STATE_ID" name="STATE_ID" class="form-control" ng-change="GetstateChange()" required>
                                                        <option value="">Select State</option>
                                                        <option ng-repeat="state in StateList" value="{{state.STATE_ID}}">{{state.STATE_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(EmployeeAddEdit.STATE_ID.$invalid && (EmployeeAddEdit.STATE_ID.$touched || EmployeeAddEdit.STATE_ID.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.STATE_ID.$error.required">State is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">City:</label><label style="color:red;">*</label>
                                                <select ng-init="CITY_ID" id="CITY_ID" class="form-control" name="CITY_ID" ng-model="CITY_ID"
                                                        ng-options="city.CITY_ID as city.CITY_NAME for city in CityList"
                                                        ng-selected="city.CITY_ID == CITY_ID" required>
                                                    <option value="">Select City</option>
                                                </select>
                                                @*<select ng-model="CITY_ID" id="CITY_ID" name="CITY_ID" class="form-control" required>
                                                        <option value="">Select City</option>
                                                        <option ng-repeat="city in CityList" value="{{city.CITY_ID}}">{{city.CITY_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(EmployeeAddEdit.CITY_ID.$invalid && (EmployeeAddEdit.CITY_ID.$touched || EmployeeAddEdit.CITY_ID.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.CITY_ID.$error.required">City is required</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">Zip Code:</label><label style="color:red;">*</label>
                                                <input type="number" id="ZIP_CODE" name="ZIP_CODE" ng-model="ZIP_CODE" ng-pattern="/^[1-9]{1}[0-9]{2}[0-9]{3}$/" onkeypress="if(this.value.length==6) return false;" max="999999" maxlength="6" onkeyup="if(parseInt(this.value)>999999){ this.value =999999; return false; }" class="form-control" placeholder="Enter Zip Code" required />
                                                <span ng-show="(EmployeeAddEdit.ZIP_CODE.$invalid && (EmployeeAddEdit.ZIP_CODE.$touched || EmployeeAddEdit.ZIP_CODE.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.ZIP_CODE.$error.required">Zip Code is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ZIP_CODE.$error.number">Enter numbers only</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ZIP_CODE.$error.pattern">Zip Code is invalid</span>
                                                </span>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">Salary Per Month:</label><label style="color:red;">*</label>
                                                <input type="number" id="SALERY_PER_MONTH" name="SALERY_PER_MONTH" maxlength="100" min="0" ng-model="SALERY_PER_MONTH" onkeyup="if(parseInt(this.value)<0){ this.value =0; return false; }" class="form-control ng-pristine ng-valid ng-touched" autocomplete="off" ng-pattern="/^([1-9]\d*)$/" placeholder="Enter Salary Per Month" required />
                                                <span ng-show="(EmployeeAddEdit.SALERY_PER_MONTH.$invalid && (EmployeeAddEdit.SALERY_PER_MONTH.$touched || EmployeeAddEdit.SALERY_PER_MONTH.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.SALERY_PER_MONTH.$error.required">Salary Per Month is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.SALERY_PER_MONTH.$error.pattern">Invalid Salary Per Month</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Married Status :</label><label style="color:red;">*</label>
                                                <input type="radio" ng-model="MARRIED_STATUS" id="MARRIED_STATUS1" name="MARRIED_STATUS" value="Unmarried" />
                                                <label for="contactChoice1">Unmarried</label>
                                                <input type="radio" ng-model="MARRIED_STATUS" id="MARRIED_STATUS2" name="MARRIED_STATUS" value="Married" />
                                                <label for="contactChoice2">Married</label>
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label class="control-label">Physical Disability :</label><label style="color:red;">*</label>
                                                <input type="radio" id="PHYSICAL_DUSABILITY1" name="PHYSICAL_DUSABILITY" ng-model="PHYSICAL_DUSABILITY" value="Yes" />
                                                <label for="contactChoice1">Yes</label>
                                                <input type="radio" id="PHYSICAL_DUSABILITY2" name="PHYSICAL_DUSABILITY" ng-model="PHYSICAL_DUSABILITY" value="No" />
                                                <label for="contactChoice2">No</label>
                                            </div>


                                            <div class="col-lg-12" style="text-align:center;margin-bottom:15px;"> <b style="color:green;font-size:18px;">Bank Details</b></div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Bank Name:</label><label style="color:red;">*</label>
                                                <input type="text" id="BANK_NAME" name="BANK_NAME" ng-model="BANK_NAME" maxlength="100" ng-pattern="/^[A-Za-z]+([\ A-Za-z]+)*/" class="form-control" placeholder="Enter Bank Name" required />
                                                <span ng-show="(EmployeeAddEdit.BANK_NAME.$invalid && (EmployeeAddEdit.BANK_NAME.$touched || EmployeeAddEdit.BANK_NAME.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.BANK_NAME.$error.required">Bank Name is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.BANK_NAME.$error.pattern">Bank Name is invalid</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Account No.:</label><label style="color:red;">*</label>
                                                <input type="number" maxlength="18" id="ACCOUNT_NO" name="ACCOUNT_NO" ng-model="ACCOUNT_NO" ng-pattern="/^[0-9][0-9]{8,17}$/" onkeypress="if(this.value.length==18) return false;" class="form-control" placeholder="Enter Account No" required />
                                                <span ng-show="(EmployeeAddEdit.ACCOUNT_NO.$invalid && (EmployeeAddEdit.ACCOUNT_NO.$touched || EmployeeAddEdit.ACCOUNT_NO.$dirty))">
                                                    <span class="error" ng-show="EmployeeAddEdit.ACCOUNT_NO.$error.required">Account No. is required</span>
                                                    <span class="error" ng-show="EmployeeAddEdit.ACCOUNT_NO.$error.pattern">Account No. is invalid</span>
                                                </span>
                                            </div>
