            //alert();
            //console.log(document.getElementById("checkAllChkbox").checked);
            $("#checkAllChkbox6").change(function () {
                if (this.checked) {
                    $(".Tools").each(function () {
                        this.checked = true;
                    });
                } else {
                    $(".Tools").each(function () {
                        this.checked = false;
                    });
                }
            });

            $(".Tools").click(function () {
                if ($(this).is(":checked")) {
                    var isAllChecked = 0;

                    $(".Tools").each(function () {
                        if (!this.checked)
                            isAllChecked = 1;
                    });

                    if (isAllChecked == 0) {
                        $("#checkAllChkbox6").prop("checked", true);
                    }
                }
                else {
                    $("#checkAllChkbox6").prop("checked", false);
                }
            });
        }

    </script>

