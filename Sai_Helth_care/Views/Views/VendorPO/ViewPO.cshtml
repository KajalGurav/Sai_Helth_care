
@{
    ViewBag.Title = "ViewPO";
}

<style>
    .catStyle_Desc {
        color: black;
        cursor: pointer;
        font-size: 15px;
        border: 1px solid green;
        padding: 5px 10px;
        border-radius: 5px;
    }

    .catStyle_active_Desc {
        color: white;
        background-color: green;
        cursor: pointer;
        font-size: 15px;
        font-weight: bold;
        padding: 5px 10px;
        border: 1px solid green;
        border-radius: 5px;
    }
    .wrapper.overflow {
        overflow-y: scroll;
        display: inline-block;
    }
</style>
<style type="text/css" media="print">
    .noprint {
        display: none;
    }
</style>

<style>
    .table td, .table th {
        padding: 0.50rem;
        vertical-align: top;
        border-top: 1px solid;
    }

    ul.no-bullets {
        list-style-type: none;
        margin: 1px;
        padding: 5px;
        align-content: center;
    }
</style>

<style type="text/css">
    body {
        font-family: Arial;
        font-size: 10pt;
    }

    .error {
        color: Red;
    }
</style>

<div class="page-content fade-in-up" ng-controller="VendorPOViewCtrl">
    <input type="hidden" id="tempCustId" />

    <div class="ibox">
        <div class="ibox-body">
            <div class="card">
                <div class="col-lg-1">
                    <a ng-click="GoToPreviousNextPage('Previous')" class="btn btn-xs" style="background-color:green;color:white"><span class="fa fa-arrow-left"></span></a>
                </div>
                <div class="card-body">
                    <div style="overflow-y:auto">
                        <table class="table table-striped table-bordered table-hover" id="example-table1" cellspacing="0" width="100%">
                            <thead style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black; ">
                                <tr>
                                    <th>PO Number</th>
                                    <th>PO Date</th>
                                    <th>Supplier Firm</th>
                                    <th>Contact Person</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody style="color:black;">
                                <tr >
                                    <td>{{VendorPODetailsList.VPO_NUMBER}}</td>
                                    <td>{{VendorPODetailsList.VPO_DATE}}</td>
                                    <td>{{VendorPODetailsList.VENDOR_NAME}}</td>
                                    <td>{{VendorPODetailsList.OWNER_NAME}}</td>
                                    <td>{{VendorPODetailsList.STATUS}}</td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-lg-12">
                        <div style="overflow: auto; white-space: nowrap; padding:15px 0px;">
                            <center>
                                <span>
                                    <span class="catmenu_button_Desc catStyle_active_Desc" id="1_catId_Desc" ng-click="OnCategoryClick('1')" style="padding:5px; margin:5px;font-size:12px;"><b>Vendor Product</b></span>
                                </span>

                            </center>
                        </div>
                    </div>
                </div>
            </div>



            <div class="card-body" id="Demoexample_1">
                <div class="ibox-body" style="height:700px; overflow-y:auto;">
                    <div class="col-lg-12" style="margin-bottom:15px;">
                        <div class="row">
                            <div id="example-table2" data-myval="5" class="table-responsive col-lg-6" style="background-color: white; height: 340px; overflow-y: scroll;">

                                <table class="table table-striped table-bordered table-hover " id="example-table3" cellspacing="0">
                                    <thead style="background-color: gray; color: white; ">
                                        <tr>
                                            <th>Product Detail</th>
                                            <th>Action</th>
                                        </tr>
                                        <tr>
                                            <th>
                                                <input style="width:150PX" id="searchInTb" class="form-control" type="text" placeholder="Search Product" />
                                            </th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody style="color:black;">
                                        <tr ng-repeat="Product in ProductList">
                                            <td>{{Product.PRODUCT_NAME}}</td>
                                            @*<td>{{Product.P_ID}}</td>*@
                                            <td>
                                                <a href="" title="Add" ng-click="Getspare(Product)" data-toggle="modal" data-target="#AddProductAccessories" class="btn btn-xs btn-success">Add </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="table-responsive col-lg-6" style="background-color: white; height: 340px; overflow-y: scroll;">
                                <table class="table table-striped table-bordered table-hover " id="example-table4" cellspacing="0" width="50%">
                                    <thead style="background-color: gray; color: white; ">
                                        <tr><td colspan="8"><strong>*Note: If main product is removed all spare parts and std accessories will be removed</strong></td></tr>
                                    </thead>
                                    <thead style="background-color: gray; color: white; ">
                                        <tr>
                                            <th>Sr. No.</th>
                                            <th>Product Name</th>
                                            <th>Is With Warranty</th>
                                            <th>Warranty Qty</th>
                                            <th>Warranty Price</th>
                                            <th>Product Qty</th>
                                            <th>Product Price</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody style="color:black;" ng-repeat="P in VendorPOProductList">

                                        <tr>
                                            <td>{{$index+1}}</td>
                                            <td>{{P.PRODUCT_NAME}}</td>
                                            <td>{{P.WITH_WARRANTY}}</td>
                                            <td>{{P.WARRANTY_QTY}}</td>
                                            <td>{{P.WARRANTY_PRICE}}</td>
                                            <td>{{P.QUANTITY}}</td>
                                            <td>{{P.PRICE}}</td>
                                            <td>
                                                <a ng-click="getFordelete(P)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>

                                        <tr ng-if="CUSTOMER_TYPE_ID===1 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 1} | filter: {'VPO_P_ID' : P.VPO_P_ID} |filter:search:strict">
                                            <td colspan="7"><b>Std Acc:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                        <tr ng-if="CUSTOMER_TYPE_ID===1 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 2} | filter: {'VPO_P_ID' : P.VPO_P_ID}  |filter:search:strict">
                                            <td colspan="7"><b>Spare Part:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                        <tr ng-if="CUSTOMER_TYPE_ID===5 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 3} | filter: {'VPO_P_ID' : P.VPO_P_ID}  |filter:search:strict">
                                            <td colspan="7"><b>Probe:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                        <tr ng-if="CUSTOMER_TYPE_ID===3 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 4} | filter: {'VPO_P_ID' : P.VPO_P_ID}  |filter:search:strict">
                                            <td colspan="7"><b>Main System:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                        <tr ng-if="CUSTOMER_TYPE_ID===3 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 5} | filter: {'VPO_P_ID' : P.VPO_P_ID}  |filter:search:strict">
                                            <td colspan="7"><b>Attachment:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                        <tr ng-if="CUSTOMER_TYPE_ID===3 && P.VendorPOProductAccessoriesList.length>0" ng-repeat="Acc in P.VendorPOProductAccessoriesList | filter: {'ACC_TYPE_ID' : 6} | filter: {'VPO_P_ID' : P.VPO_P_ID}  |filter:search:strict">
                                            <td colspan="7"><b>Tool:</b> {{Acc.ACCESSORY_NAME}}&nbsp;<b> Price:</b>{{Acc.PART_PRICE}}&nbsp; <b>Quantity:</b>{{Acc.PART_QTY}}&nbsp;</td>
                                            <td>
                                                <a ng-click="getFordelete(Acc)" title="Remove" class="btn btn-xs btn-success">Remove </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12 " style="margin-bottom:10px">
                        <div class="row">

                            <div class="form-group col-md-2">
                                <label class="control-label">Total Amount:</label>
                                <input type="text" name="AMOUNT" ng-model="TOTAL_AMOUNT" class="form-control" readonly
                                       placeholder="Total Amount" value="0" />
                            </div>
                            <div class="form-group col-md-2">
                                <label class="control-label">Inc. All Taxes:</label>
                                <label style="color:red;">*</label>
                                <select ng-model="IAT_ID" id="IAT_ID" name="IAT_ID" class="form-control"
                                        ng-options="IAT.IAT_ID as IAT.IAT_NAME for IAT in IncludingAllTaxesList"
                                        ng-selected="IAT.IAT_ID == IAT_ID" ng-change="OnIncTaxChange()" required>
                                    <option value="">Select</option>

                                </select>
                            </div>
                            <div class="form-group col-md-2">
                                <label class="control-label">GST:</label><label style="color:red;">*</label>

                                <select ng-model="GSTP_ID" id="GSTP_ID" name="GSTP_ID" class="form-control"
                                        ng-options="gst.GSTP_ID as gst.GST_PERC for gst in GSTPercentageList"
                                        ng-selected="gst.GSTP_ID == GSTP_ID" ng-change="CalculateTaxAmount()">
                                    <option value="">NA</option>

                                </select>

                            </div>
                            <div class="form-group col-md-3">
                                <label class="control-label">Tax Amount:</label>
                                <input type="text" name="TAX_AMOUNT" class="form-control" ng-model="TAX_AMOUNT" readonly placeholder="Tax Amount" />
                            </div>
                            <div class="form-group col-md-3">
                                <label class="control-label">Amount Inc. Tax:</label>
                                <input type="text" name="AMOUNT_INC_TAX" class="form-control" ng-model="AMOUNT_INC_TAX" readonly placeholder="Amount Inc. Tax" />
                            </div>

                            <div class="form-group col-md-12">
                                <label class="control-label">Terms & Conditions:</label>
                                <textarea ng-model="TERMS_AND_CONDITIONS" id="TERMS_AND_CONDITIONS" class="form-control" type="text" placeholder="Enter Terms & Conditions"></textarea>

                            </div>

                        </div>
                        <div class="flex-row" style="margin-left: 45%; margin-right: 45%; margin-top: 11px; display: flex; ">
                            <div class="flex-col" style="flex: 1; margin-right: 20px;">
                                <input id="SaveVendorPO" class="form-control" type="button" value="Save" style="background-color:green;color:black" ng-click="SavePODetails()" />
                            </div>
                            <div class="flex-col" style="flex: 1;">
                                <input id="PrintVendorPO" class="form-control" type="button" value="Print" style="background-color:aquamarine;color:black" ng-click="numTowords()" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal fade" id="AddProductAccessories">
                <div class="modal-dialog modal-xl">
                    <div class="modal-content">
                        <div class="modal-header" style="background-color:gray; color: white; padding: 10px; ">
                            <h4 class="modal-title">
                                <b> Add Product Accessories </b>
                            </h4>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form name="AddPayment" ng-submit="AddPayment(AddPayment.$valid)" data-toggle="validator" role="form">
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <div class="col-12">
                                            <div class="row">

                                                <div class="form-group col-md-2">
                                                    <label class="control-label">Quantity:<label style="color:red;">*</label></label>

                                                    <input id="prodQuant" class="form-control" type="number" name="PRODUCT_QUANTITY" ng-model="PRODUCT_QUANTITY" value="0" min="0" ng-pattern="/^([0-9]\d*)$/" onkeyup="if(parseInt(this.value)<0){ this.value =0; return false; }" required />
                                                    <span ng-show="(AddPayment.PRODUCT_QUANTITY.$invalid && (AddPayment.PRODUCT_QUANTITY.$touched || AddPayment.PRODUCT_QUANTITY.$dirty))">
                                                        <span class="error" ng-show="AddPayment.PRODUCT_QUANTITY.$error.required">Quantity is required</span>
                                                        <span class="error" ng-show="AddPayment.PRODUCT_QUANTITY.$error.number">Please enter a valid Quantity</span>
                                                    </span>
                                                </div>

                                                <div class="form-group col-md-4">
                                                    <label class="control-label">Product Price:<label style="color:red;">*</label></label>
                                                    <input id="prodPrice" class="form-control" type="number" name="PRODUCT_PRICE" ng-model="PRODUCT_PRICE" min="0" ng-pattern="/^\d+(\.\d{1,2})?$/" ng-change="formatPrice()" required />
                                                    <span ng-show="(AddPayment.PRODUCT_PRICE.$invalid && (AddPayment.PRODUCT_PRICE.$touched || AddPayment.PRODUCT_PRICE.$dirty))">
                                                        <span class="error" ng-show="AddPayment.PRODUCT_PRICE.$error.required">Product Price is required</span>
                                                        <span class="error" ng-show="AddPayment.PRODUCT_PRICE.$error.number">Please enter valid Product Price</span>
                                                        <span class="error" ng-show="AddPayment.PRODUCT_PRICE.$error.pattern">Only two decimal places are allowed</span>
                                                    </span>
                                                </div>

                                            </div>
                                            <div class="row">
                                                <div class="form-group col-md-4">
                                                    <label class="control-label">Is With Warranty:</label><label style="color:red;">*</label>

                                                    <select ng-model="IS_WITH_WARRANTY" id="IS_WITH_WARRANTY" name="IS_WITH_WARRANTY" class="form-control" required>
                                                        <option value="">Select</option>
                                                        <option value=1>Yes</option>
                                                        <option value=0>No</option>
                                                    </select>
                                                    <span ng-show="(AddPayment.IS_WITH_WARRANTY.$invalid && (AddPayment.IS_WITH_WARRANTY.$touched || AddPayment.IS_WITH_WARRANTY.$dirty))">
                                                        <span class="error" ng-show="AddPayment.IS_WITH_WARRANTY.$error.required">Is With Warranty is required</span>
                                                        <span class="error" ng-show="AddPayment.IS_WITH_WARRANTY.$error.number">Please Select  Is With Warranty</span>
                                                    </span>
                                                </div>
                                                <div class="form-group col-md-2">
                                                    <label class="control-label">Warranty Quantity:<label style="color:red;">*</label></label>

                                                    <input id="warrantyQuant" class="form-control" type="number" name="WARRANTY_QTY" ng-model="WARRANTY_QTY" value="0" min="0" ng-pattern="/^([0-9]\d*)$/" onkeyup="if(parseInt(this.value)<0){ this.value =0; return false; }" required />
                                                    <span ng-show="(AddPayment.WARRANTY_QTY.$invalid && (AddPayment.WARRANTY_QTY.$touched || AddPayment.WARRANTY_QTY.$dirty))">
                                                        <span class="error" ng-show="AddPayment.WARRANTY_QTY.$error.required">Warranty Quantity is required</span>
                                                        <span class="error" ng-show="AddPayment.WARRANTY_QTY.$error.number">Please enter a valid Warranty Quantity</span>
                                                    </span>
                                                </div>

                                                <div class="form-group col-md-4">
                                                    <label class="control-label">Warranty Price:<label style="color:red;">*</label></label>

                                                    <input id="warrantyPrice" class="form-control" type="number" name="WARRANTY_PRICE" ng-model="WARRANTY_PRICE" value="0" min="0" ng-pattern="/^([0-9]\d*)$/" onkeyup="if(parseInt(this.value)<0){ this.value =0; return false; }" required />
                                                    <span ng-show="(AddPayment.WARRANTY_PRICE.$invalid && (AddPayment.WARRANTY_PRICE.$touched || AddPayment.WARRANTY_PRICE.$dirty))">
                                                        <span class="error" ng-show="AddPayment.WARRANTY_PRICE.$error.required">Warranty Price is required</span>
                                                        <span class="error" ng-show="AddPayment.WARRANTY_PRICE.$error.number">Please enter valid Warranty Price</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <table ng-show="CUSTOMER_TYPE_ID===1" class="table table-bordered" id="AccTable1" cellspacing="0" style="font-size: 0.9rem;color:black;">

                                            <thead>
                                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                                    <th>
                                                        <input id="checkAllChkbox1" type="checkbox" value="false" onclick="CheckALLStdAcc()" />
                                                    </th>
                                                    <th>Std Accessories</th>
                                                    <th style="width:100px;">Qty</th>
                                                    <th style="width:150px;">Price</th>
                                                </tr>
                                                <tr>
                                                    <th></th>
                                                    <th>
                                                        <input id="searchStdAcc" class="form-control" type="text" placeholder="Search" />
                                                    </th>
                                                    <th></th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="std in ProductAccessoriesSparePartList | filter: {'ACC_TYPE_ID' : 1}">
                                                    <td scope="row" class="checkbox_th">
                                                        <input id="prodSpare" class="StdAcc" value="{{std.ACC_ID}}" ng-model="STD_ACC_ID" type="checkbox" />
                                                    </td>

                                                    <td>{{std.ACCESSORY_NAME}}</td>
                                                    <td><input class="form-control qid" type="number" name="Q{{std.ACC_ID}}" id="Q{{std.ACC_ID}}" value="1" min="1" max="300" ng-pattern="/^(^[-][1-9]\d*)$/" onkeyup="if(parseInt(this.value)<1){ this.value =1; return false; } if(parseInt(this.value)> this.max){ this.value = this.max; return false; }" /></td>
                                                    <td><input class="form-control priceid" type="number" name="PART_PRICE{{std.ACC_ID}}" id="PART_PRICE{{std.ACC_ID}}" value="0" step="0.01" min="0" onkeyup="if(parseFloat(this.value)<0){ this.value =0; return false; }" /></td>

                                                </tr>

                                            </tbody>
                                        </table>

                                        <table ng-show="CUSTOMER_TYPE_ID===1" class="table table-bordered" id="AccTable2" cellspacing="0" style="font-size: 0.9rem;color:black;">

                                            <thead>
                                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                                    <th>
                                                        <input id="checkAllChkbox2" type="checkbox" value="false" onclick="CheckALLSpareParts()" />
                                                    </th>
                                                    <th>Spare Parts</th>
                                                    <th style="width:100px;">Qty</th>
                                                    <th style="width:150px;">Price</th>
                                                </tr>
                                                <tr>
                                                    <th></th>
                                                    <th>
                                                        <input id="searchSparePart" class="form-control" type="text" placeholder="Search" />
                                                    </th>
                                                    <th></th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="sp in ProductAccessoriesSparePartList | filter: {'ACC_TYPE_ID' : 2}">
                                                    <td scope="row" class="checkbox_th">
                                                        <input id="prodSpare" class="SpareParts" value="{{sp.ACC_ID}}" ng-model="SPARE_PART_ID" type="checkbox" />
                                                    </td>

                                                    <td>{{sp.ACCESSORY_NAME}}</td>
                                                    <td><input class="form-control qid" type="number" name="Q{{sp.ACC_ID}}" id="Q{{sp.ACC_ID}}" value="1" min="1" max="300" ng-pattern="/^(^[-][1-9]\d*)$/" onkeyup="if(parseInt(this.value)<1){ this.value =1; return false; } if(parseInt(this.value)> this.max){ this.value = this.max; return false; }" /></td>
                                                    <td><input class="form-control priceid" type="number" name="PART_PRICE{{sp.ACC_ID}}" id="PART_PRICE{{sp.ACC_ID}}" value="0" step="0.01" min="0" onkeyup="if(parseFloat(this.value)<0){ this.value =0; return false; }" /></td>

                                                </tr>

                                            </tbody>
                                        </table>
                                        <table ng-show="CUSTOMER_TYPE_ID===5" class="table table-bordered" id="AccTable3" cellspacing="0" style="font-size: 0.9rem;color:black;">

                                            <thead>
                                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                                    <th>
                                                        <input id="checkAllChkbox3" type="checkbox" value="false" onclick="CheckALLProbes()" />
                                                    </th>
                                                    <th>Probes</th>
                                                    <th style="width:100px;">Qty</th>
                                                    <th style="width:150px;">Price</th>
                                                </tr>
                                                <tr>
                                                    <th></th>
                                                    <th>
                                                        <input id="searchProbe" class="form-control" type="text" placeholder="Search" />
                                                    </th>
                                                    <th></th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="probe in ProductAccessoriesSparePartList | filter: {'ACC_TYPE_ID' : 3}">
                                                    <td scope="row" class="checkbox_th">
                                                        <input id="prodSpare" class="MainSystem" value="{{probe.ACC_ID}}" ng-model="MAIN_SYSTEM_ID" type="checkbox" />
                                                    </td>

                                                    <td>{{probe.ACCESSORY_NAME}}</td>
                                                    <td><input class="form-control qid" type="number" name="Q{{probe.ACC_ID}}" id="Q{{probe.ACC_ID}}" value="1" min="1" max="300" ng-pattern="/^(^[-][1-9]\d*)$/" onkeyup="if(parseInt(this.value)<1){ this.value =1; return false; } if(parseInt(this.value)> this.max){ this.value = this.max; return false; }" /></td>
                                                    <td><input class="form-control priceid" type="number" name="PART_PRICE{{probe.ACC_ID}}" id="PART_PRICE{{probe.ACC_ID}}" value="0" step="0.01" min="0" onkeyup="if(parseFloat(this.value)<0){ this.value =0; return false; }" /></td>

                                                </tr>

                                            </tbody>
                                        </table>
                                        <table ng-show="CUSTOMER_TYPE_ID===3" class="table table-bordered" id="AccTable4" cellspacing="0" style="font-size: 0.9rem;color:black;">

                                            <thead>
                                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                                    <th>
                                                        <input id="checkAllChkbox4" type="checkbox" value="false" onclick="CheckALLMainSystem()" />
                                                    </th>
                                                    <th>Main System</th>
