  border: 1px solid black;
  margin-bottom: 20px;
  margin-top: 20px;
  width: 100%;
}

.table-container img {
  width: 100%;
  object-fit: contain;
  max-width: 500px;
}
img {
    width:400px;
    margin-block-start:15px ;
    margin: auto;
    display: block;
    margin-block-end:15px ;
    max-width: 100%;
}
.bordered {
    border-radius: 30px 30px 0 0;
    border-top-style: solid;
    /* border-top-width: 1px; */
    border-left-style: solid;
    border-color: #a5a9ad;
    /* border-left-width: 1px; */
    border-right-style: solid;
    /* border-right-width: 1px; */
    width: 100%;
    padding: 20px;
    display: flow-root;
}
/* Media query for print mode */
@@media print {
  .table-container{
    overflow: hidden;
  }
  img {
    width:400px;
    margin-block-start:15px ;
    margin: auto;
    display: block;
    margin-block-end:15px ;
    max-width: 100%;
}


}
table {
    width: 100%;
    border-collapse: collapse;
