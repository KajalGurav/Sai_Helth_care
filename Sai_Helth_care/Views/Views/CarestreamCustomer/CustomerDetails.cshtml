                                                                        <div class="table-responsive">
                                                                            <div class="col-12">
                                                                                <div class="row">

                                                                                    <div class="form-group col-md-4">
                                                                                        <label class="control-label">Quotation Number:</label>
                                                                                        <input type="text" id="QUOTATION_NO" name="QUOTATION_NO"  class="form-control " ng-model="QUOTATION_NO" placeholder="Quotation Number" readonly />
                                                                                        
                                                                                    </div>
                                                                                    <div class="form-group col-md-4">
                                                                                        <label class="control-label">P.O. Date:</label>
                                                                                        <label style="color:red;">*</label>
                                                                                        <input type="text" id="PO_DATE" name="PO_DATE" maxlength="10" class="form-control ng-pristine ng-valid ng-touched" ng-model="PO_DATE" placeholder="Select P.O. date" required />
                                                                                        <span ng-show="(AddPayment1.PO_DATE.$invalid && (AddPayment1.PO_DATE.$touched || AddPayment1.PO_DATE.$dirty))">
                                                                                            <span class="error" ng-show="AddPayment1.PO_DATE.$error.required">P.O. Date is required</span>
                                                                                        </span>
                                                                                    </div>
                                                                                    <div class="form-group col-md-4">
                                                                                        <label class="control-label">Amount without TAX:</label>
                                                                                        <label style="color:red;">*</label>
                                                                                        <input class="form-control" type="number" name="AMOUNT_WITHOUT_TAX" ng-model="AMOUNT_WITHOUT_TAX" min="0" ng-pattern="/^([1-9]\d*)$/" ng-change="CalculateTAX()" onkeyup="if(parseInt(this.value)<0){ this.value =0; return false; }" />
                                                                                        <span ng-show="(AddPayment1.AMOUNT_WITHOUT_TAX.$invalid && (AddPayment1.AMOUNT_WITHOUT_TAX.$touched || AddPayment1.AMOUNT_WITHOUT_TAX.$dirty))">
                                                                                            <span class="error" ng-show="AddPayment1.AMOUNT_WITHOUT_TAX.$error.required">Amount without TAX is required</span>
                                                                                            <span class="error" ng-show="AddPayment1.AMOUNT_WITHOUT_TAX.$error.number">Please enter valid Amount without TAX</span>
                                                                                        </span>
                                                                                    </div>
                                                                                    <div class="form-group col-md-4">
                                                                                        <label class="control-label">TAX Amount:</label>
                                                                                        <input class="form-control" type="number" name="TAX_AMOUNT" ng-model="TAX_AMOUNT" min="0" ng-pattern="/^([1-9]\d*)$/" readonly />
                                                                                        <span ng-show="(AddPayment1.TAX_AMOUNT.$invalid && (AddPayment1.TAX_AMOUNT.$touched || AddPayment1.TAX_AMOUNT.$dirty))">
                                                                                            <span class="error" ng-show="AddPayment1.TAX_AMOUNT.$error.required">Amount without TAX is required</span>
                                                                                            <span class="error" ng-show="AddPayment1.TAX_AMOUNT.$error.number">Please enter valid Amount without TAX</span>
                                                                                        </span>
                                                                                    </div>
                                                                                    <div class="form-group col-md-4">
                                                                                        <label class="control-label">Amount Inc. TAX:</label>
                                                                                        <input class="form-control" type="number" name="AMOUNT_WITH_TAX" ng-model="AMOUNT_WITH_TAX" min="0" ng-pattern="/^([1-9]\d*)$/" readonly />
                                                                                        <span ng-show="(AddPayment1.AMOUNT_WITH_TAX.$invalid && (AddPayment1.AMOUNT_WITH_TAX.$touched || AddPayment1.AMOUNT_WITH_TAX.$dirty))">
                                                                                            <span class="error" ng-show="AddPayment1.AMOUNT_WITH_TAX.$error.required">Amount without TAX is required</span>
                                                                                            <span class="error" ng-show="AddPayment1.AMOUNT_WITH_TAX.$error.number">Please enter valid Amount without TAX</span>
