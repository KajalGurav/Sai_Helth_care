                                        <select ng-model="P_STOCK_ID" id="PSERIAL_NO_ID" name="PSERIAL_NO_ID" ng-change="SelectProductSerialNo()" class="form-control"
                                                ng-selected="srno.P_STOCK_ID == PSERIAL_NO_ID">
                                            <option value="">Select Product Serial No</option>
                                            <option ng-repeat="srno in StockProductSerialNoList" ng-value="{{srno.P_STOCK_ID}}">{{srno.P_SERIAL_NO}}</option>
