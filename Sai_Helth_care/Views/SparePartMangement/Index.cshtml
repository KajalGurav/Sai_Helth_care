                                    </div>*@

                                    <div class="form-group col-md-6" ng-show="STOCK_NO!='NULL'">
                                        <label class="control-label">Product Serial No:</label><label style="color:red;">*</label>
                                        <input type="text" name="Product Serial No" class="form-control" ng-model="P_SERIAL_NO" placeholder="Product Serial No" />
                                    </div>

                                    <div class="form-group col-md-6" ng-show="PRODUCT_TYPE === 'Medtronic'">
                                        <label class="control-label">Batch No:</label><label style="color:red;">*</label>
                                        <input type="text" name="Batch No" class="form-control" ng-model="BATCH_NO" placeholder="Enter Batch No" ng-required="PRODUCT_TYPE == 'Medtronic'" />
                                    </div>

                                    <div class="form-group col-md-6">
                                        <label class="control-label">Select Part Type:</label><label style="color:red;">*</label>
                                        <select ng-model="PART_TYPE_ID" id="PART_TYPE_ID" class="form-control" ng-change="GetPartChange()"
                                                ng-options="type.PART_TYPE_ID as type.PART_TYPE_NAME for type in PartTypeList"
                                                ng-selected="type.PART_TYPE_ID == PART_TYPE_ID" required>
                                            <option value="">Select Part Type</option>

                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Part Name:</label><label style="color:red;">*</label>
                                        <select ng-model="PART_ID" id="PART_ID" name="PART_ID" class="form-control"
                                                ng-options="part.PART_ID as part.PART_NAME for part in PartList"
                                                ng-selected="part.PART_ID == PART_ID" required>
                                            <option value="">Select Part Name</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Part Serial No:</label><label style="color:red;">*</label>
                                        <input type="text" name="Part Serial No" class="form-control" ng-model="PART_SERIAL_NO" placeholder="Enter Part Serial No" required />
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Part No:</label>
                                        <input type="text" name="Part No" class="form-control" ng-model="PART_NO" placeholder="Enter Part No" />
                                    </div>

                                    <div class="form-group col-md-6">
                                        <label class="control-label">Quantity:</label><label style="color:red;">*</label>
                                        <input type="text" name="Author Name" class="form-control" ng-model="PART_QTY" placeholder="Enter Quantity" required />
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Stock Entry Date:</label><label style="color:red;">*</label>
                                        <input type="text" name="Stock Entry Date" class="form-control" ng-model="STOCK_ENTRY_DATE" id="STOCK_ENTRY_DATE" placeholder="Enter Stock Entry Date" />
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Status:</label><label style="color:red;">*</label>
                                        <select class="form-control" ng-model="STATUS" required>
                                            <option value="">Select Status</option>
                                            <option value="NA">NA</option>
                                            <option value="Need To Check">Need To Check</option>
                                            <option value="Defective">Defective</option>
                                            <option value="Working">Working</option>
                                            <option value="Not Working">Not Working</option>
                                            <option value="Scrap">Scrap</option>

                                        </select>

                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Part Price:</label><label style="color:red;">*</label>
                                        <input type="text" name="PART_PRICE" class="form-control" ng-pattern="/^(0*([1-9]\d*?|0)(\.\d{1,2})?)$/" ng-model="PART_PRICE" placeholder="Enter Part Price" required />
                                        <span style="color:red" ng-if="SPM.PART_PRICE.$error.pattern">Please enter valid number</span>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label class="control-label">Location:</label>
                                        <input type="text" name="Location" class="form-control" ng-model="LOCATION" placeholder="Enter Location" />
                                    </div>

                                    <div class="form-group col-md-6">
                                        <label class="control-label">HSN No:</label>
                                        <input type="text" name="HSN No" class="form-control" ng-model="HSN_CODE" placeholder="Enter HSN No" />
                                    </div>

                                    <div class="form-group col-md-6">
                                        <label class="control-label">Remark:</label><label style="color:red;">*</label>
                                        <textarea type="text" name="Remark" class="form-control" ng-model="REMARK" placeholder="Enter Remark" required></textarea>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <div class="card-footer">
                            <div class="form-group col-md-12">
                                <div class="form-group col-md-12">
                                    <input type="button" class="btn btn-sm" style="background-color:forestgreen; color: white; font-size: 14px;" ng-disabled="SPM.$invalid" ng-click="AddUpdateAdmin('ADD')" value="Add Stock" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>

