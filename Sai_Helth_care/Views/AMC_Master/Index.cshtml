
@{
    ViewBag.Title = "Index";
}


<style>
    .table td, .table th {
        padding: 0.50rem;
        vertical-align: top;
        border-top: 1px solid #dee2e6;
        font-size: 12px;
    }

    body {
        font-family: Arial;
        font-size: 10pt;
    }

    .error {
        color: Red;
    }
</style>

<style>
    #printECRPAF {
        display: block;
    }

    #border1 {
        display: none;
    }

    .paf {
        text-shadow: 5px 5px 8px #666;
    }
</style>
<style type="text/css" media="print">
    input[type="checkbox"] {
        visibility: hidden;
    }

    input[type="checkbox"] {
        border: 1px solid #333;
        content: "\00a0";
        display: inline-block;
        font: 16px/1em sans-serif;
        height: 16px;
        margin: 0 .25em 0 0;
        padding: 0;
        vertical-align: top;
        width: 16px;
    }

        input[type="checkbox"]:checked {
            background: white;
            color: black;
            content: "\2713";
            text-align: center;
        }
</style>

<style>
    .table td, .table th {
        padding: 0.50rem;
        vertical-align: top;
        border-top: 1px solid #dee2e6;
        font-size: 12px;
    }

    #example-table thead {
        background-color: #BE7833;
        position: sticky;
        z-index: 1024;
        color: white;
        top: 0;
    }

    .table-responsive {
        max-height: 400px;
        overflow-y: auto;
    }
</style>



<section ng-controller="AMCCtrl">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-1">
                                <a ng-click="GoToPreviousNextPage('Previous')" class="btn btn-xs" style="background-color: green; color: white "><span class="fa fa-arrow-left"></span></a>
                            </div>
                            <div class="col-lg-9" style="margin-left:-50px">
                                <h3 class="card-title"><b>AMC/CMC Registration</b></h3>
                            </div>
                            <div class="col-lg-2" style="text-align:center">
                                <button type="button" class="btn btn-sm pull-left" style="background-color: green; color: white" data-toggle="modal" ng-click="AdminClick()">Add Record</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-3" ng-show="@Session["COMPANY_ID"]==0">
                            <div class="form-group">
                                <select ng-model="COMPANY_ID" id="COMPANY_ID" name="COMPANY_ID" class="form-control"
                                        ng-options="c.COMPANY_ID as c.COMPANY_NAME for c in CompanyList"
                                        ng-selected="c.COMPANY_ID == COMPANY_ID" ng-change="SearchAdmin()">
                                    <option value="">Select Company</option>
                                </select>
                            </div>
                        </div>


                        <div class="col-lg-2" style="text-align:center">
                            <a href="~/AMC_Master/UpcomingPmCm?CustType={{CUSTOMER_TYPE_ID}}" type="button" class="form-control btn btn-sm pull-left" style="background-color: green; color: white ">
                                <div style="margin-top: 5px;">Upcoming Pm/Cm</div>
                            </a>
                        </div>
                        <div class="col-lg-2" style="text-align:center">
                            <a href="~/AMC_Master/UpcomingExpiry" type="button" class="form-control btn btn-sm pull-left" style="background-color: green; color: white; ">
                                <div style="margin-top: 5px;">Upcoming Expiry</div>
                            </a>
                        </div>
                        <div class="col-1"></div>
                        <div class="col-3">
                            <input type="text" class="form-control" placeholder="Search Doc.No./Customer/Firm" ng-model="FARMER_SEARCH" ng-keyup="SearchAdmin()">
                        </div>
                    </div>
                    <br>
                    <div class="table-responsive">
                        <table class="table table-bordered" id="example-table" style="font-size: 0.9rem;color:black;">
                            <thead>
                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                    <th>Sr. No.</th>
                                    <th>Doc. No.</th>
                                    <th>Contract Period(Years)</th>
                                    <th>Contract Period from</th>
                                    <th>Contract Period To</th>
                                    <th>Customer Name</th>
                                    <th>Firm Name</th>
                                    <th ng-hide="CUSTOMER_TYPE_NAME==='Medtronic'">Product Name</th>
                                    <th>Model No/Name.</th>
                                    <th>Serial No.</th>
                                    <th>PM Visit</th>
                                    <th>CM Visit</th>
                                    <th>Contract Type</th>
                                    <th>Contract Status</th>
                                    <th>Visit Dates</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="admin in AMCCMCList">
                                    <td>{{page*30+$index+1}}</td>
                                    <td>{{admin.CONTRACT_DOCUMENT_NO}}</td>
                                    <td>{{admin.CONTRACT_PERIOD}}</td>
                                    <td>{{admin.CONTRACT_FROM}}</td>
                                    <td>{{admin.CONTRACT_TO}}</td>
                                    <td>{{admin.CUSTOMER_NAME}}</td>
                                    <td>{{admin.CUSTOMER_FIRM_NAME}}</td>
                                    <td ng-hide="CUSTOMER_TYPE_NAME==='Medtronic'">{{admin.PRODUCT_NAME}}</td>
                                    <td>{{admin.MODEL_NAME}}</td>
                                    <td>{{admin.MODEL_SERIAL_NO}}</td>
                                    <td>{{admin.PM_VISIT}}</td>
                                    <td>{{admin.CM_VISIT}}</td>
                                    <td>{{admin.CONTRACT_TYPE}}</td>
                                    <td ng-show="admin.AMC_CMC_STATUS=='Done'"><b style="color:green">{{admin.AMC_CMC_STATUS}}</b></td>
                                    <td ng-show="admin.AMC_CMC_STATUS=='Pending'"><b style="color:red">{{admin.AMC_CMC_STATUS}}</b></td>
                                    <td>
                                        <span ng-repeat="visitDate in admin.VisitDates.split(',')">
                                            <span ng-show="visitDate.includes('*')" style="background-color:red;color:white;">{{ visitDate.trim() }} <br></span>
                                            <span ng-hide="visitDate.includes('*')">{{ visitDate.trim() }} <br></span>
                                        </span>
                                    </td>
                                    <td>
                                        <a data-toggle="modal" ng-click="getForUpdate(admin)" title="Update" class="btn btn-xs btn-danger">Edit </a>
                                        <a ng-show="admin.CONTRACT_TYPE!='Warranty'" data-toggle="modal" ng-click="PreviewPR(admin)" title="Update" class="btn btn-xs btn-success">Print Preview </a>

                                    </td>
                                </tr>
                                <tr ng-show="CUSTOMER_TYPE_NAME!='Medtronic' && (!pager.pages.length)">
                                    <td colspan="16" style="color:black; text-align:center;"><h5><b>No record found!</b></h5></td>
                                </tr>
                                <tr ng-show="CUSTOMER_TYPE_NAME=='Medtronic' && (!pager.pages.length)">
                                    <td colspan="15" style="color:black; text-align:center;"><h5><b>No record found!</b></h5></td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                    <ul ng-if="pager.pages.length" class="pagination" style="margin-left:30px;">
                        <li ng-class="{disabled:pager.currentPage === 1}">
                            <a ng-click="setPage(1)">First</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === 1}">
                            <span ng-if="pager.currentPage!=1"><a ng-click="setPage(pager.currentPage - 1)">Previous</a></span>
                            <span ng-if="pager.currentPage==1">Previous</span>
                        </li>
                        <li ng-repeat="page in pager.pages" ng-class="{active:pager.currentPage === page}">
                            <a ng-click="setPage(page)">{{page}}</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === pager.totalPages}">
                            <a ng-click="setPage(pager.currentPage + 1)">Next</a>
                        </li>
                        <li ng-class="{disabled:pager.currentPage === pager.totalPages}">
                            <a ng-click="setPage(pager.totalPages)">Last</a>
                        </li>
                    </ul>
                </div>

                <div class="modal fade" id="AMC_AddUpdate">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header" style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: white; padding: 10px; ">

                                <h4 class="modal-title"><b>{{Admin_Action}} @*AMC/CMC Registration*@  </b></h4>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <div class="card-body">

                                    <form name="AddPayment" ng-submit="AddPayment(AddPayment.$valid)" data-toggle="validator" role="form">
                                        <div class="row">
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Contract Document No:</label><label style="color:red;">*</label>
                                                <input type="text" name="CONTRACT_DOCUMENT_NO" ng-model="CONTRACT_DOCUMENT_NO" class="form-control" readonly placeholder="Contract Document No" />
                                                <span ng-show="(AddPayment.CONTRACT_DOCUMENT_NO.$invalid && (AddPayment.CONTRACT_DOCUMENT_NO.$touched || AddPayment.CONTRACT_DOCUMENT_NO.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CONTRACT_DOCUMENT_NO.$error.required">Contract Document No is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label"> Select Contract Type:</label><label style="color:red;">*</label>
                                                <select name="CONTRACT_TYPE" class="form-control" selected="{{CONTRACT_TYPE}}" ng-model="CONTRACT_TYPE" required>
                                                    <option value="">Select Contract Name</option>
                                                    <option value="AMC">AMC</option>
                                                    <option value="CMC">CMC</option>
                                                    <option value="Warranty">Warranty</option>
                                                </select>
                                                <span ng-show="(AddPayment.CONTRACT_TYPE.$invalid && (AddPayment.CONTRACT_TYPE.$touched || AddPayment.CONTRACT_TYPE.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CONTRACT_TYPE.$error.required">Contract Type is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label"> Contract Period (Year): </label><label style="color:red;">*</label>

                                                <select name="CONTRACT_PERIOD" id="CONTRACT_PERIOD" class="form-control" ng-model="CONTRACT_PERIOD" required>
                                                    <option value="">Select Contract Period (Year)</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                    <option value="5">5</option>
                                                    <option value="6">6</option>
                                                    <option value="7">7</option>
                                                    <option value="8">8</option>
                                                    <option value="9">9</option>
                                                    <option value="10">10</option>
                                                </select>
                                                <span ng-show="(AddPayment.CONTRACT_PERIOD.$invalid && (AddPayment.CONTRACT_PERIOD.$touched || AddPayment.CONTRACT_PERIOD.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CONTRACT_PERIOD.$error.required">Contract Period (Year) is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Contract Date : </label><label style="color:red;">*</label>
                                                <input type="text" id="CONTRACT_DATE" name="CONTRACT_DATE" ng-model="CONTRACT_DATE" class="form-control" maxlength="10" @*readonly*@ placeholder="Contract Date" />
                                                @*<span ng-show="(AddPayment.CONTRACT_DATE.$invalid && (AddPayment.CONTRACT_DATE.$touched || AddPayment.CONTRACT_DATE.$dirty))">
                                                        <span class="error" ng-show="AddPayment.CONTRACT_DATE.$error.required">Contract Date is required</span>
                                                        <span class="error" ng-show="AddPayment.CONTRACT_DATE.$error.date">Invalid Contract Date</span>
                                                    </span>*@
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Select Customer Name: </label><label style="color:red;">*</label>
                                                <select name="CUSTOMER_ID" id="CUSTOMER_ID" class="form-control" ng-model="CUSTOMER_ID"
                                                        ng-options="Customer.Customer_ID as Customer.CUSTOMER_NAME for Customer in CustomerList"
                                                        ng-selected="Customer.Customer_ID == CUSTOMER_ID" ng-change="GetCustomerChange()" required>
                                                    <option value="">Select Customer Name</option>
                                                </select>
                                                <span ng-show="(AddPayment.CUSTOMER_ID.$invalid && (AddPayment.CUSTOMER_ID.$touched || AddPayment.CUSTOMER_ID.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CUSTOMER_ID.$error.required">Customer Name is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Select Customer Firm Name:</label><label style="color:red;">*</label>
                                                <select name="FIRM_ID" id="FIRM_ID" class="form-control" ng-model="FIRM_ID"
                                                        ng-options="Customer.F_ID as Customer.FIRM_NAME for Customer in CustomerFirmList"
                                                        ng-selected="Customer.F_ID == FIRM_ID" ng-change="GetFirmChange()" required>
                                                    <option value="">Select Customer Firm Name </option>
                                                    @*<option ng-repeat="Customer in CustomerFirmList" value="{{Customer.F_ID}}">{{Customer.FIRM_NAME}}</option>*@
                                                </select>
                                                <span ng-show="(AddPayment.FIRM_ID.$invalid && (AddPayment.FIRM_ID.$touched || AddPayment.FIRM_ID.$dirty))">
                                                    <span class="error" ng-show="AddPayment.FIRM_ID.$error.required">Customer Firm Name is required</span>
                                                </span>
                                            </div>
                                            <div ng-hide="CUSTOMER_TYPE_NAME=='Medtronic'" class="form-group col-md-6">
                                                <label class="control-label">Select Product:</label><label style="color:red;">*</label>
                                                <select name="CAT_ID" id="CAT_ID" class="form-control" ng-model="CAT_ID"
                                                        ng-options="Category.CAT_ID as Category.CAT_NAME for Category in CategoryList"
                                                        ng-selected="Category.CAT_ID == CAT_ID" ng-change="GetCategoryChange()" ng-required="CUSTOMER_TYPE_NAME!='Medtronic'">
                                                    <option value="">Select Product</option>
                                                </select>
                                                @*<select name="CAT_ID" id="CAT_ID" class="form-control" ng-model="CAT_ID" ng-change="GetCategoryChange()" required>
                                                        <option value="">Select Product</option>
                                                        <option ng-repeat="Category in CategoryList" value="{{Category.CAT_ID}}">{{Category.CAT_NAME}}</option>
                                                    </select>*@
                                                <span ng-show="(AddPayment.CAT_ID.$invalid && (AddPayment.CAT_ID.$touched || AddPayment.CAT_ID.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CAT_ID.$error.required">Product is required</span>
                                                </span>
                                            </div>

                                            <div @*ng-show="CAT_ID!=7"*@ class="form-group col-md-6">
                                                <label class="control-label">Select Model:</label><label style="color:red;">*</label>
                                                <select name="P_ID" id="P_ID" class="form-control" ng-model="P_ID"
                                                        @*ng-options="option.value as option.label for option in getOptions()"
                                                        ng-selected="option.value == P_ID" ng-change="GetModelChange()" required>*@
                                                        ng-options="Product.P_ID as Product.PRODUCT_NAME for Product in ProductList"
                                                        ng-selected="Product.P_ID == P_ID" ng-change="GetModelChange()" required>
                                                    <option value="">Select Model</option>
                                                </select>
                                                <span ng-show="(AddPayment.P_ID.$invalid && (AddPayment.P_ID.$touched || AddPayment.P_ID.$dirty))">
                                                    <span class="error" ng-show="AddPayment.P_ID.$error.required">Model is required</span>
                                                </span>
                                            </div>
                                            @*<div ng-show="CAT_ID==7" class="form-group col-md-6">
                                                    <label class="control-label">Select Model:</label><label style="color:red;">*</label>
                                                    <select name="P_ID" id="P_ID" class="form-control" ng-model="P_ID"
                                                            ng-options="Product.MP_ID as Product.PRODUCT_NAME for Product in ProductList"
                                                            ng-selected="Product.MP_ID == P_ID" ng-change="GetModelChange()" required>
                                                        <option value="">Select Model</option>
                                                    </select>
                                                    <span ng-show="(AddPayment.P_ID.$invalid && (AddPayment.P_ID.$touched || AddPayment.P_ID.$dirty))">
                                                        <span class="error" ng-show="AddPayment.P_ID.$error.required">Model is required</span>
                                                    </span>
                                                </div>*@
                                            <div class="form-group col-md-6" ng-show="CUSTOMER_TYPE_NAME!='Medtronic' && CONTRACT_TYPE == 'CMC' && (CAT_ID == 1 || CAT_ID == 3 || CAT_ID == 5)">
                                                <label class="control-label"> Select Contract Type Details:</label><label ng-show="CUSTOMER_TYPE_NAME!='Medtronic' && CONTRACT_TYPE == 'CMC' && (CAT_ID == 1 || CAT_ID == 3 || CAT_ID == 5)" style="color:red;">*</label>
                                                <select name="CONTRACT_TYPE_DETAILS" class="form-control" ng-selected="{{CONTRACT_TYPE_DETAILS}}" ng-model="CONTRACT_TYPE_DETAILS" ng-required="CUSTOMER_TYPE_NAME!='Medtronic' && CONTRACT_TYPE == 'CMC' && (CAT_ID == 1 || CAT_ID == 3 || CAT_ID == 5)">
                                                    <option value="">Select Contract Type Details</option>
                                                    <option ng-show="CAT_ID == 1" value="With Tube">With Tube</option>
                                                    <option ng-show="CAT_ID == 1" value="Without Tube">Without Tube</option>
                                                    <option ng-show="CAT_ID == 3" value="Including Helium">Including Helium</option>
                                                    <option ng-show="CAT_ID == 3" value="Excluding Helium">Excluding Helium</option>
                                                    <option ng-show="CAT_ID == 5" value="With 1 Probe">With 1 Probe</option>
                                                    <option ng-show="CAT_ID == 5" value="With 2 Probes">With 2 Probes</option>
                                                    <option ng-show="CAT_ID == 5" value="With 3 Probes">With 3 Probes</option>
                                                    <option ng-show="CAT_ID == 5" value="With 4 Probes">With 4 Probes</option>
                                                    <option ng-show="CAT_ID == 5" value="Without Probes">Without Probes</option>

                                                </select>
                                                <span ng-show="(AddPayment.CONTRACT_TYPE_DETAILS.$invalid && (AddPayment.CONTRACT_TYPE_DETAILS.$touched || AddPayment.CONTRACT_TYPE_DETAILS.$dirty))">
                                                    <span class="error" ng-show="AddPayment.CONTRACT_TYPE_DETAILS.$error.required">Contract Type Details is required</span>
                                                </span>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label class="control-label">Model Serial No :</label>@*<label style="color:red;">*</label>*@
                                                <input type="text" name="MODEL_SERIAL_NO" class="form-control" ng-model="MODEL_SERIAL_NO" placeholder="Enter Model Serial." @*required*@ />
                                                <span ng-show="(AddPayment.MODEL_SERIAL_NO.$invalid && (AddPayment.MODEL_SERIAL_NO.$touched || AddPayment.MODEL_SERIAL_NO.$dirty))">
                                                    <span class="error" ng-show="AddPayment.MODEL_SERIAL_NO.$error.required">Model Serial No is required</span>
                                                </span>
                                            </div>


                                            <div ng-show="CUSTOMER_TYPE_NAME=='Medtronic'" class="form-group col-md-12" style="margin:0px; padding:0px;">
                                                <div class="row" style="margin:0px; padding:0px;">
                                                    <div class="form-group col-md-6">
                                                        <label class="control-label">DC For:</label>
                                                        <select name="DC_For" class="form-control" ng-model="AMC_For">
                                                            <option value="MainSystem">Main System</option>
                                                            <option value="Attachments">Attachments</option>
                                                            <option value="Tools">Tools</option>
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-md-6" ng-show="AMC_For === 'MainSystem'">
                                                        <label class="control-label">Main System:</label><label style="color:red;">*</label>
                                                        <select ng-model="MED_ACC_ID" id="MED_ACC_ID1" name="MED_ACC_ID1" class="form-control"
                                                                ng-options="accs.MED_ACC_ID as accs.ACCESSORY_NAME for accs in AccessoriesList | filter: {'MED_ACCESSORY_TYPE_ID' : 1} |filter:search:strict"
                                                                ng-selected="accs.MED_ACC_ID == MED_ACC_ID">
                                                            <option value="">Select Main System</option>
                                                        </select>
                                                        <span ng-show="IsMainSystemSelected == 'No'" style="color:red">Please select Main System</span>
                                                    </div>
                                                    <div class="form-group col-md-6" ng-show="AMC_For === 'Attachments'">
                                                        <label class="control-label">Attachments:</label><label style="color:red;">*</label>
                                                        <select ng-model="MED_ACC_ID" id="MED_ACC_ID2" name="MED_ACC_ID2" class="form-control"
                                                                ng-options="accs.MED_ACC_ID as accs.ACCESSORY_NAME for accs in AccessoriesList | filter: {'MED_ACCESSORY_TYPE_ID' : 2} |filter:search:strict"
                                                                ng-selected="accs.MED_ACC_ID == MED_ACC_ID">
                                                            <option value="">Select Attachments</option>
                                                        </select>
                                                        <span ng-show="IsAttachmentsSelected == 'No'" style="color:red">Please select Attachments</span>
                                                    </div>
                                                    <div class="form-group col-md-6" ng-show="AMC_For === 'Tools'">
                                                        <label class="control-label">Tools:</label><label style="color:red;">*</label>
                                                        <select ng-model="MED_ACC_ID" id="MED_ACC_ID3" name="MED_ACC_ID3" class="form-control"
                                                                ng-options="accs.MED_ACC_ID as accs.ACCESSORY_NAME for accs in AccessoriesList | filter: {'MED_ACCESSORY_TYPE_ID' : 3} |filter:search:strict"
                                                                ng-selected="accs.MED_ACC_ID == MED_ACC_ID">
                                                            <option value="">Select Tools</option>
                                                        </select>
                                                        <span ng-show="IsToolsSelected == 'No'" style="color:red">Please select Tools</span>
                                                    </div>
                                                </div>
                                                <div class="row" style="margin:0px; padding:0px;">
                                                    <div class="form-group col-md-4">
                                                        <label class="control-label">Part Serial No.:</label>@*<label style="color:red;">*</label>*@
                                                        <input type="text" name="PART_SERIAL_NO" ng-model="PART_SERIAL_NO" class="form-control" placeholder="Enter Part Serial No." @*required*@ />
                                                    </div>
                                                    <div class="form-group col-md-2">
                                                        <label class="control-label">Quantity:</label>@*<label style="color:red;">*</label>*@
                                                        <input type="number" id="PART_QTY" name="PART_QTY" ng-model="PART_QTY" class="form-control" step="1" min="0" max="999999" onkeyup="if (this.value < min) { this.value = min; return false; } if (this.value > max) { this.value = max; return false; } " onkeypress="if (this.value.length == 6) { return false; }"
                                                               placeholder="Enter Qty" @*required*@ />
                                                    </div>
                                                    <div class="form-group col-md-4">
                                                        <label class="control-label">Price:</label>@*<label style="color:red;">*</label>*@
                                                        <input type="number" id="PART_PRICE" name="PART_PRICE" ng-model="PART_PRICE" class="form-control" step="0.01" min="0" max="9999999" onkeyup="if (this.value < min) { this.value = min; return false; } if (this.value > max) { this.value = max; return false; } " onkeypress="if (this.value.length == 7) { return false; }" placeholder="Enter Price" @*required*@ />
                                                    </div>
                                                    <div class="form-group col-md-2">
                                                        <label class="control-label" style="color:white">.</label>
                                                        <button type="button" class="btn btn-sm pull-left form-control"
                                                                style="background-color:peru; color: white"
                                                                ng-click="AddPartsButtonClicked()">
                                                            Add Parts
                                                        </button>
                                                    </div>
                                                </div>


                                                <div class="form-group col-md-12">
                                                    <div class="table-responsive">
                                                        <table class="table table-bordered" style="font-size: 0.9rem;color:black;">
                                                            <thead>
                                                                <tr style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black ">
                                                                    <th style="width:20px;">No.</th>
                                                                    <th>Accessories</th>
                                                                    <th>Serial No.</th>
